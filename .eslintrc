{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["plugin:prettier/recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-unused-vars": 2,
    "no-undef": 2
  }
}
